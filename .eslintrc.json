{
    "env": {
        "browser": true,
        "es2021": true,
        "node": true
    },
    "extends": [
        "airbnb-base"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
        "no-console":"off",
        "linebreak-style":"off",
        "no-underscore-dangle":"off",
        "import/no-extraneous-dependencies":"off",
        "no-undef":"off",
        "consistent-return":"off",
        "no-prototype-builtins":"off",
        "import/prefer-default-export":"off",
        "no-new":"off",
        "class-methods-use-this":"off"
    }
}
